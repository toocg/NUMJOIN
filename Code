#include <bits/stdc++.h>
using namespace std;
int n;
string a[1000];
bool cmp1(string x,string y){          // ham sort giam dan trong mang (maybe idk)
return x+y>y+x;
}
main(){
ios_base::sync_with_stdio(false);
cin.tie(0);
cout.tie(0);
cin >> n;
for(int i=1;i<=n;i++){
    cin >> a[i];
}
sort(a+1,a+1+n,cmp1);
for(int i=1;i<=n;i++){
    cout << a[i];
}
}
